    "import torch\n",
    "x=torch.tensor([1.0],requires_grad=True)\n",
    "y1=x**2\n",
    "\n",
    "with torch.no_grad():\n",
    "    y2=x**3\n",
    "y3=y1+y2\n",
    "y3.backward()\n",
    "print(x.grad)"
